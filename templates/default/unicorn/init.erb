# sinatra      Startup script for Sinatra server.
# description: Starts Sinatra as an unprivileged user.
#

cd <%= @site_dir %> && /usr/local/bin/bundle exec /usr/local/bin/unicorn_rails -D -c <%= @config_file %>
RETVAL=$?

exit $RETVAL